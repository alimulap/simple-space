[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://src-rust/target/debug/libsimple-space.so"
linux.release.x86_64 =   "res://src-rust/target/release/libsimple-space.so"
windows.debug.x86_64 =   "res://src-rust/target/debug/simple_space.dll"
windows.release.x86_64 = "res://src-rust/target/release/simple_space.dll"
macos.debug =            "res://src-rust/target/debug/libsimple_space.dylib"
macos.release =          "res://src-rust/target/release/libsimple-space.dylib"
macos.debug.arm64 =      "res://src-rust/target/debug/libsimple-space.dylib"
macos.release.arm64 =    "res://src-rust/target/release/libsimple-space.dylib"

